{
  "name": "ameliorate/nexus",
  "description": "A package to setup a nexus of stops which are processed based on the result of the previous stop.",
  "type": "package",
  "keywords": ["nexus"],
  "license": "MIT",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Exilesprx",
      "email": "campbell.andrew86@yahoo.com"
    }
  ],
  "require": {
    "illuminate/contracts": "^5.5",
    "illuminate/support": "^5.5",
    "php": "7.1.*",
    "mockery/mockery": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Ameliorate\\": "Ameliorate/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "spec\\": "spec/",
      "tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "require-dev": {
    "fzaninotto/faker": "^1.7",
    "phpspec/phpspec": "^4.3",
    "phpunit/phpunit": "^7"
  }
